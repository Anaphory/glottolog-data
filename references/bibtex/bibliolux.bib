@article{house_constructing_2006,
    author = {House, Jill},
    title = {Constructing a context with intonation},
    journal = {Journal of Pragmatics},
    volume = {38},
    number = {10},
    pages = {1542--1558},
    year = {2006},
    issn = {0378-2166},
    url = {http://www.sciencedirect.com/science/article/pii/S0378216606000385},
    abstract = {It is widely accepted that intonation makes a significant and systematic contribution to utterance interpretation. Less well understood are the linguistic or paralinguistic mechanisms by which the meaningful effects we identify and describe are communicated. Recent work by Gussenhoven (2002) illuminates how these functions may be reconciled by appealing to partly grammaticalised ‘biological codes’. An explanatory pragmatic framework for analysing intonational meaning from the hearer's perspective may be found in Relevance Theory (Sperber and Wilson, 1986/1995), which allows linguistic expressions to be coded for either conceptual or procedural meaning (Blakemore, 2002). This paper investigates what procedural coding, typically involving constraints on inferential processes, might look like when applied to intonation. Discussion centres on the structure-building properties of intonation together with an examination of the range of functions associated with one particular tonal pattern, the high rising tone ({HRT}).},
    date = {2006-10},
    doi = {10.1016/j.pragma.2005.07.005},
    file = {House - 2006 - Constructing a context with intonation.pdf:/Users/peter.gilles/Documents/zotero/storage/HTTJZCS7/House - 2006 - Constructing a context with intonation.pdf:application/pdf;ScienceDirect Snapshot:/Users/peter.gilles/Documents/zotero/storage/TSKG624Z/S0378216606000385.html:text/html},
    glottolog_ref_id = {470472},
    keywords = {High rising tone, Intonation, {PRAGMATICS}, Prosody, Relevance Theory},
    lgcode = {Luxembourgish [ltz]},
    shortjournal = {Journal of Pragmatics},
    urldate = {2014-03-14}
}
@article{house_english_2003,
    author = {House, Juliane},
    title = {English as a lingua franca: A threat to multilingualism?},
