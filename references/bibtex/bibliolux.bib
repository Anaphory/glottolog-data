@article{hall_alveolopalatalization_2012,
    author = {Hall, T. A.},
    title = {Alveolopalatalization in Central German as Markedness Reduction},
    journal = {Transactions of the Philological Society},
    pages = {n/a--n/a},
    year = {2012},
    issn = {1467-968X},
    url = {http://onlinelibrary.wiley.com/doi/10.1111/1467-968X.12002/abstract},
    abstract = {The present study investigates the historical unconditioned merger of palatal /ç/ ({\textless} Middle High German /x/) and postalveolar /ʃ/ into alveolopalatal /ɕ/ in certain varieties of Central German. It will be shown that the phonemicization of the alveolopalatal was the final stage in a gradual reduction of segmental markedness. Thus, /ç/ and /ʃ/ are argued to be featurally complex segments, while /ɕ/ is featurally simplex. Independent support for the proposed analysis can be found in the concomitant reduction of markedness in the vowel system of the dialects in question. The present study sheds light on the factors involved in explaining context-free phonological change and the historical source of the alveolopalatal /ɕ/.},
    doi = {10.1111/1467-968X.12002},
    file = {Full Text PDF:/Users/peter.gilles/Documents/zotero/storage/P8TNE7JJ/Hall - 2012 - Alveolopalatalization in Central German as Markedn.pdf:application/pdf;Snapshot:/Users/peter.gilles/Documents/zotero/storage/SNNS2AUF/full.html:text/html},
    glottolog_ref_id = {470147},
    inlg = {English [eng]},
    lgcode = {Luxembourgish [ltz]},
    rights = {© The author 2013. Transactions of the Philological Society © The Philological Society 2013.},
    urldate = {2013-01-10}
}
