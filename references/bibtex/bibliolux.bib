@article{montgomery_effect_2012,
    author = {Montgomery, Chris},
    title = {The effect of proximity in perceptual dialectology},
    journal = {Journal of Sociolinguistics},
    volume = {16},
    number = {5},
    pages = {638--668},
    year = {2012},
    issn = {1467-9841},
    url = {http://onlinelibrary.wiley.com/doi/10.1111/josl.12003/abstract},
    abstract = {This article examines the effect of proximity on perceptual dialectological data gathered in Northern Great Britain. It underlines the key effect that proximity has on respondent mental maps of language variation, and demonstrates this by discussing area recognition levels and map-based data. The article demonstrates that the effects of physical proximity can be modified by various factors. These factors include the ‘barrier effect’ of national borders such as the Scottish-English border, along with other, more subjective, psychological boundaries such as the North-South divide. In addition to demonstrating that these barrier effects increase the effect of proximity, this article will also show that proximity effects can be reduced by factors such as ‘cultural prominence’ (in which culturally important locations feature more prominently in mental maps of dialect areas).},
    doi = {10.1111/josl.12003},
    file = {Montgomery - 2012 - The effect of proximity in perceptual dialectology.pdf:/Users/peter.gilles/Documents/zotero/storage/WMXB33NC/Montgomery - 2012 - The effect of proximity in perceptual dialectology.pdf:application/pdf;Montgomery - 2012 - The effect of proximity in perceptual dialectology.pdf:/Users/peter.gilles/Documents/zotero/storage/TGRPTTPE/Montgomery - 2012 - The effect of proximity in perceptual dialectology.pdf:application/pdf;Snapshot:/Users/peter.gilles/Documents/zotero/storage/AKCHQXTH/abstract.html:text/html;Snapshot:/Users/peter.gilles/Documents/zotero/storage/7PHMFET5/abstract.html:text/html},
    glottolog_ref_id = {471583},
    inlg = {English [eng]},
    keywords = {dialect areas, folk linguistics, language variation, Perceptual dialectology, proximity},
    lgcode = {Luxembourgish [ltz]},
    rights = {© Blackwell Publishing Ltd. 2012},
    urldate = {2013-01-10}
}
