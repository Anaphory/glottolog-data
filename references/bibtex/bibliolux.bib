@article{kager_stress_2012,
    author = {Kager, René},
    title = {Stress in windows: Language typology and factorial typology},
    journal = {Lingua},
    volume = {122},
    number = {13},
    pages = {1454--1493},
    year = {2012},
    issn = {0024-3841},
    url = {http://www.sciencedirect.com/science/article/pii/S0024384112001301},
    abstract = {This paper will draw up a survey of metrical window systems informed by typological databases. The survey confirms that the maximum window size equals three syllables at both edges. Only final syllables (not initial ones) can be obligatorily unstressed. Any syllable in initial and final windows can be a default position. Three metrical models will be tested with respect to these generalizations: the first using binary feet and symmetrical extrametricality; the second, a grid-only model with anti-lapse constraints; the third, weakly layered ternary feet. Only the latter two models are able to generate the full attested typology. Yet only the weakly layered model avoids overgenerations known as ‘midpoint pathology’. The consequences for metrical theory will be discussed.},
    date = {2012-10},
    doi = {10.1016/j.lingua.2012.06.005},
    file = {ScienceDirect Full Text PDF:/Users/peter.gilles/Documents/zotero/storage/QAT2ZD69/Kager - 2012 - Stress in windows Language typology and factorial.pdf:application/pdf;ScienceDirect Snapshot:/Users/peter.gilles/Documents/zotero/storage/CC29NSD4/S0024384112001301.html:text/html},
    glottolog_ref_id = {470737},
    keywords = {Factorial typology, Metrical typology, Optimality theory, Phonology, Stress windows},
    lgcode = {Luxembourgish [ltz]},
    shortjournal = {Lingua},
    shorttitle = {{SI}: Varieties of Pitch Accent Systems},
    urldate = {2013-06-20}
}
