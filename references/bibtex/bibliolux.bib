@article{wilson_relevance_2006,
    author = {Wilson, Deirdre and Wharton, Tim},
    title = {Relevance and prosody},
    journal = {Journal of Pragmatics},
    volume = {38},
    number = {10},
    pages = {1559--1579},
    year = {2006},
    issn = {0378-2166},
    url = {http://www.sciencedirect.com/science/article/pii/S0378216606000397},
    abstract = {Prosodic elements such as stress and intonation are generally seen as providing both ‘natural’ and properly linguistic input to utterance comprehension. They contribute not only to overt communication but to more covert or accidental forms of information transmission. They typically create impressions, convey information about emotions or attitudes, or alter the salience of linguistically-possible interpretations rather than conveying distinct propositions or concepts in their own right. These aspects of communication present a challenge to pragmatic theory: how should they be described and explained? This paper is an attempt to explore how the wealth of insights provided by the literature on the interpretation of prosody might be integrated into the relevance-theoretic framework (Sperber and Wilson, 1986/1995; Blakemore, 2002; Carston, 2002). We will focus on four main issues. First, how should the communication of emotions, attitudes and impressions be analysed? Second, how might prosodic elements function as ‘natural’ communicative devices? Third, what (if anything) do prosodic elements encode? Fourth, what light can the study of prosody shed on the place of pragmatics in the architecture of the mind? In each case, we hope to show that the study of prosody and the study of pragmatics can interact in ways that benefit both disciplines.},
    date = {2006-10},
    doi = {10.1016/j.pragma.2005.04.012},
    file = {ScienceDirect Snapshot:/Users/peter.gilles/Documents/zotero/storage/GV8BCV64/S0378216606000397.html:text/html;Wilson and Wharton - 2006 - Relevance and prosody.pdf:/Users/peter.gilles/Documents/zotero/storage/KGR2ASIF/Wilson and Wharton - 2006 - Relevance and prosody.pdf:application/pdf},
    glottolog_ref_id = {472920},
    keywords = {Inference, {PRAGMATICS}, Prosody, Relevance Theory, Signals, Signs},
    lgcode = {Luxembourgish [ltz]},
    shortjournal = {Journal of Pragmatics},
    urldate = {2014-03-14}
}
