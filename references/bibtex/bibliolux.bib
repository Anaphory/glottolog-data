@article{deuber_dynamics_2007,
    author = {{DEUBER}, {DAGMAR} and {HINRICHS}, {LARS}},
    title = {Dynamics of orthographic standardization in Jamaican Creole and Nigerian Pidgin},
    journal = {World Englishes},
    volume = {26},
    number = {1},
    pages = {22--47},
    year = {2007},
    issn = {1467-971X},
    url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1467-971X.2007.00486.x/abstract;jsessionid=FF1F68B5CE2B594B052F658A47601C9E.d04t03?userIsAuthenticated=false&deniedAccessCustomisedMessage=},
    abstract = {{ABSTRACT}:  Jamaican Creole and Nigerian Pidgin, both English-derived contact languages that coexist with standard English ({StE}) in their respective settings, are traditionally oral languages for which no orthographic standard has yet been established. Aided by a significant increase in written usage in computer-mediated communication ({CMC}), the past decade has seen tendencies towards grass-roots standardization in the orthography of these two languages. In this paper we (1) identify the dynamics of orthographic standardization for each of the varieties using corpora of {CMC} data (in particular e-mails and discussion forums posts), (2) discuss the differential results for the two varieties based on the different sociolinguistic situations of Jamaica and Nigeria, and (3) point to possible consequences for language planning in the two countries. We conclude that, without any state-driven initiative toward the standardization of Pidgin/Creole orthography, the gap between the orthographic systems usually recommended by linguists, or “experts” (cf. Sebba, 1998a), and the practices that are being established by “users” is likely to widen further.},
    date = {2007-02-01},
    doi = {10.1111/j.1467-971X.2007.00486.x},
    file = {Snapshot:/Users/peter.gilles/Documents/zotero/storage/TXX8M3ZM/abstract\;jsessionid=FF1F68B5CE2B594B052F658A47601C9E.html:text/html},
    glottolog_ref_id = {469361},
    inlg = {English [eng]},
    lgcode = {Luxembourgish [ltz]},
    urldate = {2012-03-18}
}
