@article{kelly-holmes_convergence_2009,
    author = {Kelly-Holmes, Helen and Moriarty, Máiréad and Pietikäinen, Sari},
    title = {Convergence and divergence in Basque, Irish and Sámi media language policing},
    journal = {Language Policy},
    volume = {8},
    number = {3},
    pages = {227--242},
    year = {2009},
    issn = {1568-4555, 1573-1863},
    url = {http://link.springer.com.proxy.bnl.lu/article/10.1007/s10993-009-9126-y},
    abstract = {The language policies adopted, imposed, or rejected in minority language media highlight the complexities of multilingualism and its regulation or ordering in contemporary contexts. In this article, we discuss convergence and divergence in the language policing of three minority language media contexts, namely Basque, Irish and Sámi. All of the cases illustrate a shift from a clear-cut dichotomy of top-down versus bottom-up language policies to more fluid and situated notions of normativity. Furthermore, we can observe in these cases a shift from the idea that speakers of a particular language form a homogenous speech community, which can be served by one medium, to a polycentric concept of audience. However, despite these observable shifts, top-down policing, and monoglot-driven concepts of multilingualism and of speech communities still persist in all of the case studies, and many of the tensions highlighted arise from this co-existence, but so too do many opportunities.},
    date = {2009-08-01},
    doi = {10.1007/s10993-009-9126-y},
    glottolog_ref_id = {470798},
    inlg = {English [eng]},
    keywords = {Applied Linguistics, Heteroglossia, Language Education, Language policing, Minority language media, Multilingualism, Political Science, Sociolinguistics},
    lgcode = {Luxembourgish [ltz]},
    shortjournal = {Lang Policy},
    urldate = {2013-08-29}
}
