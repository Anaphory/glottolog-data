@article{scapoli_surnames_2007,
    author = {Scapoli, Chiara and Mamolini, Elisabetta and Carrieri, Alberto and Rodriguez-Larralde, Alvaro and Barrai, Italo},
    title = {Surnames in Western Europe: A comparison of the subcontinental populations through isonymy},
    journal = {Theoretical Population Biology},
    volume = {71},
    number = {1},
    pages = {37--48},
    year = {2007},
    issn = {0040-5809},
    url = {http://www.sciencedirect.com/science/article/pii/S0040580906000773},
    abstract = {We studied the isonymic structure of Western Europe using the distributions of 26.2 million surnames in 8 countries, 125 regions and 2094 towns of the Subcontinent. We found that, for the whole of Western Europe, Nei's distance was correlated with geographic distance ( r = 0.610 ± 0.009 ). It was observed that at long geographic distances the isonymyc distance stays below linearity and tends to become asymptotic, and this was attributed to long distance migration. A dendrogram of the125 regions was built and the clusters identified by the dendrogram are almost exactly coincident with the nations of the Subcontinent. Random inbreeding calculated from isonymy, {FST}, was highest in Spanish regions, and lowest in France. The geographical distribution of α in 2094 towns, high in the Center and East of the Subcontinent and lower in Spain, is compatible with the settlement of subsequent waves of migrants moving from the West and from the South toward the centre of the Continent. The present surname structure of Western Europe is strictly linked to local languages.},
    date = {2007-02},
    doi = {10.1016/j.tpb.2006.06.010},
    file = {ScienceDirect Snapshot:/Users/peter.gilles/Documents/zotero/storage/HUF3CRAV/S0040580906000773.html:text/html},
    glottolog_ref_id = {468472},
    lgcode = {Luxembourgish [ltz]},
    shortjournal = {Theoretical Population Biology},
    shorttitle = {Surnames in Western Europe},
    urldate = {2014-08-24}
}
