@article{clopper_effects_2011,
    author = {Clopper, Cynthia G. and Smiljanic, Rajka},
    title = {Effects of gender and regional dialect on prosodic patterns in American English},
    journal = {Journal of phonetics},
    volume = {39},
    number = {2},
    pages = {237--245},
    year = {2011},
    issn = {0095-4470},
    url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3113495/},
    abstract = {While cross-dialect prosodic variation has been well established for many languages, most variationist research on regional dialects of American English has focused on the vowel system. The current study was designed to explore prosodic variation in read speech in two regional varieties of American English: Southern and Midland. Prosodic dialect variation was analyzed in two domains: speaking rate and the phonetic expression of pitch movements associated with accented and phrase-final syllables. The results revealed significant effects of regional dialect on the distributions of pauses, pitch accents, and phrasal-boundary tone combinations. Significant effects of talker gender were also observed on the distributions of pitch accents and phrasal-boundary tone combinations. The findings from this study demonstrate that regional and gender identity features are encoded in part through prosody, and provide further motivation for the close examination of prosodic patterns across regional and social varieties of American English.},
    date = {2011-04-01},
    doi = {10.1016/j.wocn.2011.02.006},
    file = {PubMed Central Full Text PDF:/Users/peter.gilles/Documents/zotero/storage/AKF6VWA4/Clopper and Smiljanic - 2011 - Effects of gender and regional dialect on prosodic.pdf:application/pdf},
    glottolog_ref_id = {469103},
    lgcode = {Luxembourgish [ltz]},
    pmcid = {PMC3113495},
    pmid = {21686317},
    shortjournal = {J Phon},
    urldate = {2013-04-05}
}
@article{clopper_nationwide_2006,
    author = {Clopper, Cynthia G. and Pisoni, David B.},
    title = {The Nationwide Speech Project: A new corpus of American English dialects},
    journal = {Speech communication},
    volume = {48},
    number = {6},
    pages = {633--644},
    year = {2006},
    issn = {0167-6393},
    url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3060775/},
    abstract = {Perceptual and acoustic research on dialect variation in the United States requires an appropriate corpus of spoken language materials. Existing speech corpora that include dialect variation are limited by poor recording quality, small numbers of talkers, and/or small samples of speech from each talker. The Nationwide Speech Project corpus was designed to contain a large amount of speech produced by male and female talkers representing the primary regional varieties of American English. Five male and five female talkers from each of six dialect regions in the United States were recorded reading words, sentences, passages, and in interviews with an experimenter, using high quality digital recording equipment in a sound-attenuated booth. The resulting corpus contains nearly an hour of speech from each of the 60 talkers that can be used in future research on the perception and production of dialect variation.},
    date = {2006-06-01},
    doi = {10.1016/j.specom.2005.09.010},
    file = {PubMed Central Full Text PDF:/Users/peter.gilles/Documents/zotero/storage/C8E2KWT9/Clopper and Pisoni - 2006 - The Nationwide Speech Project A new corpus of Ame.pdf:application/pdf},
    glottolog_ref_id = {469102},
    lgcode = {Luxembourgish [ltz]},
    pmcid = {PMC3060775},
    pmid = {21423815},
    shortjournal = {Speech Commun},
    shorttitle = {The Nationwide Speech Project},
    urldate = {2013-04-05}
}
