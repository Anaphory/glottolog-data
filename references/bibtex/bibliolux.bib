@article{meunier_vowel_2011,
    author = {Meunier, Christine and Espesser, Robert},
    title = {Vowel reduction in conversational speech in French: The role of lexical factors},
    journal = {Journal of Phonetics},
    volume = {39},
    number = {3},
    pages = {271--278},
    year = {2011},
    issn = {0095-4470},
    url = {http://www.sciencedirect.com/science/article/pii/S0095447010000951},
    abstract = {{\textless}p{\textgreater}{\textless}br/{\textgreater}In this study we investigate vowel reduction and the role of some lexical factors in the production of vowels extracted from a corpus of French conversations. Vowel durations and spectral quality are examined with respect to (1) their interaction in the corpus, (2) the position of vowels in words, and (3) word frequency and word category. The analyses are conducted on vowels produced by 16 speakers. Our study provides strong evidence that vowel reduction (decrease in durations and more centralized spectral values) affects most of the vowels in conversational speech. The results show that vowels in final syllables of words were less often reduced while the preceding ones show reduced durations and centralized formant values. Moreover vowels are more reduced in monosyllabic function words than in monosyllabic content words. Nevertheless, we did not find a clear effect of word frequency on vowel durations. Finally, our study shows that vowel reduction depends on several factors related to lexical properties (word category) and to prosodic properties (stress and final lengthening).{\textless}/p{\textgreater}},
    date = {2011-07},
    doi = {16/j.wocn.2010.11.008},
    file = {ScienceDirect Full Text PDF:/Users/peter.gilles/Documents/zotero/storage/PJ7XMHPB/Meunier and Espesser - 2011 - Vowel reduction in conversational speech in French.pdf:application/pdf;ScienceDirect Snapshot:/Users/peter.gilles/Documents/zotero/storage/QIRGUCR9/S0095447010000951.html:text/html},
    glottolog_ref_id = {469576},
    lgcode = {Luxembourgish [ltz]},
    shorttitle = {Vowel reduction in conversational speech in French},
    urldate = {2011-08-11}
}
